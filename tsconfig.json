{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true
  },
  "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ],
    "@data/*": [
      "src/data/*"
    ],
    "@layouts/*": [
      "src/layouts/*"
    ],
    "@pages/*": [
      "src/pages/*"
    ],
    "@styles/*": [
      "src/styles/*"
    ],
    "@utils/*": [
      "src/utils/*"
    ],
    "@lib/*": [
      "src/lib/*"
    ]
  },
}